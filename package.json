    "uuid": "^7.0.1",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/got": "^9.6.9",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
